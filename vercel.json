{
  "version": 2,
  "builds": [
    {
      "src": "node/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "react-front/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "react-front/build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "node/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/react-front/build"
    }
  ]
}
